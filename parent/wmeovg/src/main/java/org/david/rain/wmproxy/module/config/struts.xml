<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!-- 所有的package继承wmeovg-default-->
	<package name="config" namespace="/config" extends="wmeovg-default">
		<!-- 系统配置 -->
		<action name="clientinfo_*"  class="org.david.rain.wmproxy.module.config.action.ClientInfoAction" method="{1}"/>
		<action name="whitelist_*"  class="org.david.rain.wmproxy.module.config.action.WhiteListAction" method="{1}"/>
		<action name="game_*"  class="org.david.rain.wmproxy.module.config.action.GameAction" method="{1}"/>
	</package>   
</struts>