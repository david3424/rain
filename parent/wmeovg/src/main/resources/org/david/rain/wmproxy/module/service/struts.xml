<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!-- 所有的package继承wmeovg-default-->
	<package name="service" namespace="/service" extends="wmeovg-default">
		<!-- 系统配置 -->
		<action name="prizelog_*"  class="org.david.rain.wmproxy.module.service.action.PrizeLogAction" method="{1}"/>
		<action name="queueBrowser_*"  class="org.david.rain.wmproxy.module.service.action.QueueBrowserAction" method="{1}"/>
		<action name="serviceManager_*"  class="org.david.rain.wmproxy.module.service.action.ServiceManagerAction" method="{1}"/>
	</package>   
</struts>